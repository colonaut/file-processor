{
  "name": "file-processor",
  "version": "0.0.1",
  "description": "Upload files to your browser and process them (upload, shrink, ...) by passing process functions or request objects",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colonaut/file-processor.git"
  },
  "keywords": [
    "file",
    "process",
    "processor"
  ],
  "author": "colonaut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colonaut/file-processor/issues"
  },
  "homepage": "https://github.com/colonaut/file-processor#readme",
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "jest": "^22.1.4",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-form": "^7.2.1"
  },
  "jest": {
    "testRegex": "\\.spec\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts}",
      "!**/node_modules/**",
      "!**/out/**"
    ]
  }
}
